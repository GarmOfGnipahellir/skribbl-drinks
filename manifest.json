{
  "manifest_version": 2,
  "name": "Skribbl Drinks",
  "version": "0.1.0",

  "description": "Adds drink counting to Skribbl.io",

  "icons": {
    "48": "icons/beer-48.png",
    "128": "icons/beer-128.png"
  },

  "content_scripts": [
    {
      "matches": ["*://skribbl.io/*"],
      "js": ["dist/index.js"]
    }
  ],

  "permissions": ["*://skribbl.io/*"]
}
